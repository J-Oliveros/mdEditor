{
  "compilerOptions": {
    "target": "es2020",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmitOnError": false,
    "noEmit": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "module": "es6",
    "experimentalDecorators": true,
    "paths": {
      "mdeditor/tests/*": [
        "tests/*"
      ],
      "mdeditor/*": [
        "app/*",
        "lib/ember-json-tree/app/*",
        "lib/ember-leaflet-table/app/*",
        "lib/mdeditor-sciencebase/app/*"
      ],
      "ember-json-tree": [
        "lib/ember-json-tree/addon"
      ],
      "ember-json-tree/*": [
        "lib/ember-json-tree/addon/*"
      ],
      "ember-json-tree/test-support": [
        "lib/ember-json-tree/addon-test-support"
      ],
      "ember-json-tree/test-support/*": [
        "lib/ember-json-tree/addon-test-support/*"
      ],
      "ember-leaflet-table": [
        "lib/ember-leaflet-table/addon"
      ],
      "ember-leaflet-table/*": [
        "lib/ember-leaflet-table/addon/*"
      ],
      "ember-leaflet-table/test-support": [
        "lib/ember-leaflet-table/addon-test-support"
      ],
      "ember-leaflet-table/test-support/*": [
        "lib/ember-leaflet-table/addon-test-support/*"
      ],
      "mdeditor-sciencebase": [
        "lib/mdeditor-sciencebase/addon"
      ],
      "mdeditor-sciencebase/*": [
        "lib/mdeditor-sciencebase/addon/*"
      ],
      "mdeditor-sciencebase/test-support": [
        "lib/mdeditor-sciencebase/addon-test-support"
      ],
      "mdeditor-sciencebase/test-support/*": [
        "lib/mdeditor-sciencebase/addon-test-support/*"
      ],
      "*": [
        "types/*"
      ]
    }
  },
  "include": [
    "app/**/*",
    "tests/**/*",
    "types/**/*",
    "lib/ember-json-tree/**/*",
    "lib/ember-leaflet-table/**/*",
    "lib/mdeditor-sciencebase/**/*"
  ]
}
